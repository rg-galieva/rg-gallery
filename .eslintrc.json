{
  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "env": {
    "browser": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "no-restricted-syntax": [
      "error",
      "ForInStatement"
    ],
    "no-console": [
      "error",
      {
        "allow": [
          "error"
        ]
      }
    ]
  }
}
